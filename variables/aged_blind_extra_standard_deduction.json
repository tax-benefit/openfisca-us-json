      "file_path": "/openfisca_us/variables/finance/tax/outputs.py",
      "parameters": [
        "tax.deductions.standard.aged_or_blind.age_threshold",
        "tax.deductions.standard.aged_or_blind.amount"
      ],
      "source_code": "def formula(tax_unit, period, parameters):\n    STD = parameters(period).tax.deductions.standard\n    MARS = tax_unit(\"MARS\", period)\n    MARSType = MARS.possible_values\n    num_extra_stded = (\n        tax_unit(\"blind_head\", period) * 1\n        + tax_unit(\"blind_spouse\", period) * 1\n        + (tax_unit(\"age_head\", period) >= STD.aged_or_blind.age_threshold)\n        * 1\n        + (\n            (MARS == MARSType.JOINT)\n            & (\n                tax_unit(\"age_spouse\", period)\n                >= STD.aged_or_blind.age_threshold\n            )\n        )\n        * 1\n    )\n    extra_stded = num_extra_stded * STD.aged_or_blind.amount[MARS]\n    return extra_stded\n",
      "start_line_number": 36,
      "stop_line_number": 56,
      "variables": [
        "MARS",
        "age_head",
        "age_spouse",
        "blind_head",
        "blind_spouse"
      ]
    }
